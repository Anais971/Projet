<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".Fragment2">

    <!-- TODO: Update blank fragment layout -->

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/fond3c"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#3F0539BC"
        android:fontFamily="@font/genshin1"
        android:text="CONTROLE LED"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#FDFDFD"
        android:textSize="60sp"
        android:typeface="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.059" />

    <TextView
        android:id="@+id/Mess"
        android:layout_width="149dp"
        android:layout_height="54dp"
        android:fontFamily="@font/genshin2"
        android:textColor="#1F1D1D"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cc"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="@+id/myImageView2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.57" />


    <Button
        android:id="@+id/sendbt"
        android:layout_width="274dp"
        android:layout_height="49dp"
        android:fontFamily="@font/genshin2"
        android:text="Envoyer"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Mess"
        app:layout_constraintVertical_bias="0.32" />

    <ImageView
        android:id="@+id/myImageView2"
        android:layout_width="285dp"
        android:layout_height="512dp"
        android:adjustViewBounds="true"
        android:rotation="0"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/sendbt"
        app:layout_constraintDimensionRatio=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.13999999"
        app:srcCompat="@drawable/color2" />

    <View
        android:id="@+id/cc"
        android:layout_width="123dp"
        android:layout_height="124dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/sendbt"
        app:layout_constraintDimensionRatio="3:3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.69"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myImageView2" />

    <Button
        android:id="@+id/cryo"
        android:layout_width="90dp"
        android:layout_height="58dp"
        android:backgroundTint="#850539BC"
        android:text="Button"
        app:icon="@drawable/blable"
        app:iconSize="40dp"
        app:iconTint="#A7E5E8"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/hydro"
        app:layout_constraintStart_toStartOf="@+id/hydro"
        app:layout_constraintTop_toBottomOf="@+id/hydro"
        app:layout_constraintVertical_bias="0.08"

        />

    <Button
        android:id="@+id/hydro"
        android:layout_width="90dp"
        android:layout_height="58dp"
        android:backgroundTint="#850539BC"
        android:text="Button"
        app:icon="@drawable/bleg"
        app:iconPadding="8dp"
        app:iconSize="40dp"
        app:iconTint="#11F5FD"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.18"

        />

    <Button
        android:id="@+id/electro"
        android:layout_width="90dp"
        android:layout_height="58dp"
        android:backgroundTint="#850539BC"
        android:text="Button"
        app:icon="@drawable/violet"
        app:iconSize="40dp"
        app:iconTint="#DCB9FD"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.23000002"

        />

    <Button
        android:id="@+id/dendro"
        android:layout_width="90dp"
        android:layout_height="58dp"
        android:backgroundTint="#850539BC"
        android:text="d"
        app:icon="@drawable/vertg"
        app:iconSize="40dp"
        app:iconTint="#A1D429"


        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/anemo"
        app:layout_constraintStart_toStartOf="@+id/anemo"
        app:layout_constraintTop_toBottomOf="@+id/anemo"
        app:layout_constraintVertical_bias="0.09"

        />

    <Button
        android:id="@+id/anemo"
        android:layout_width="90dp"
        android:layout_height="58dp"
        android:backgroundTint="#850539BC"
        android:text="a"
        app:icon="@drawable/vertble"
        app:iconSize="40dp"
        app:iconTint="#B1FDD8"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/cryo"
        app:layout_constraintStart_toStartOf="@+id/cryo"
        app:layout_constraintTop_toBottomOf="@+id/cryo"
        app:layout_constraintVertical_bias="0.08"

        />

    <Button
        android:id="@+id/geo"
        android:layout_width="90dp"
        android:layout_height="58dp"
        android:backgroundTint="#850539BC"
        android:text="g"
        app:icon="@drawable/orange"
        app:iconSize="40dp"
        app:iconTint="#F9DB5C"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/pyro"
        app:layout_constraintStart_toStartOf="@+id/pyro"
        app:layout_constraintTop_toBottomOf="@+id/pyro"
        app:layout_constraintVertical_bias="0.100000024"


        />

    <Button
        android:id="@+id/pyro"
        android:layout_width="90dp"
        android:layout_height="58dp"
        android:backgroundTint="#850539BC"
        android:text="p"
        app:icon="@drawable/rougg"
        app:iconSize="40dp"
        app:iconTint="#DF8B4F"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/electro"
        app:layout_constraintStart_toStartOf="@+id/electro"
        app:layout_constraintTop_toBottomOf="@+id/electro"
        app:layout_constraintVertical_bias="0.100000024"
        />


</androidx.constraintlayout.widget.ConstraintLayout>